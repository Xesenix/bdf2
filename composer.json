{
	"name": "xesenix/bdf2",

	"description": "Black Dragon Framework 2 - Admin base template",

	"type": "project",

	"authors": [
		{
			"name": "PaweÅ‚ Kapalla",
			"email": "pkapalla@xesenix.pl",
