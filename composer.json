		"symfony/finder": "2.3.*",
		"symfony/validator": "~2.3",
		"symfony/config": "dev-master as 2.5.x-dev",
		"symfony/translation": "~2.3",
		"symfony/locale": "~2.3",
		"symfony/security-csrf": "~2.4",
		"kriswallsmith/assetic" : "~1.1",
		"natxet/CssMin": "~3",
		"resources/jquery/js/jquery": "1.11.*",
		"resources/jquery-ui/js/jquery-ui": "1.11.*",
        "resources/bootstrap": "3.2.*",
		"resources/tinymce": "4.*"
	},

	"autoload": {
		"psr-4": {
			"": "lib/",
			"BDF2\\": "lib/BDF2",
			"RPG\\": "src/"
		}
