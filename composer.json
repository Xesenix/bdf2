{
	"name": "xesenix/bdf2",

	"description": "Black Dragon Framework 2 - Admin base template",

	"type": "project",

	"authors": [
		{
			"name": "Pawe≈Ç Kapalla",
			"homepage": "http://xesenix.pl",
			"role": "Developer"
		}
	],

	"repositories": [
		{
			"type": "package",
			"package": {
				"name": "resources/jquery",
				"target-dir": "js/jquery",
				"version": "1.11.1",
				"dist": {
					"url": "http://code.jquery.com/jquery-1.11.1.min.js",
					"type": "file"
				}
			}
		}, {
			"type": "package",
			"package": {
				"name": "resources/jquery-ui",
				"target-dir": "js/jquery-ui",
				"version": "1.11.0",
				"dist": {
					"url": "http://jqueryui.com/resources/download/jquery-ui-1.11.0.zip",
					"type": "zip"
				}
			}
		}, {
			"type": "package",
			"package": {
				"name": "resources/tinymce",
				"version": "4.1.9",
				"dist": {
					"url": "http://download.moxiecode.com/tinymce/tinymce_4.1.9_jquery.zip",
					"type": "zip"
				}
			}
		}, {
			"type": "package",
			"package": {
				"name": "resources/bootstrap",
				"version": "3.3.4",
				"dist": {
					"url": "https://github.com/twbs/bootstrap/releases/download/v3.3.4/bootstrap-3.3.4-dist.zip",
					"type": "zip"
				}
			}
		}, {
			"type": "package",
			"package": {
				"name": "resources/vis",
				"version": "3.11.0",
				"dist": {
					"url": "https://github.com/almende/vis/archive/v3.11.0.zip",
					"type": "zip"
				}
			}
		}, {
			"type": "package",
			"package": {
				"name": "resources/require",
				"target-dir": "js",
				"version": "2.1.16",
				"dist": {
					"url": "http://requirejs.org/docs/release/2.1.16/minified/require.js",
					"type": "file"
				}
			}
		}, {
			"type": "package",
			"package": {
				"name": "resources/angularjs",
				"target-dir": "js",
				"version": "1.3.14",
				"dist": {
					"url": "https://github.com/components/angular.js/archive/1.3.13.zip",
					"type": "zip"
				}
			}
		}, {
			"type": "package",
			"package": {
				"name": "resources/angularjs-hint",
				"target-dir": "js",
				"version": "0.1.0",
				"dist": {
					"url": "https://github.com/angular/angular-hint/archive/v0.1.0.zip",
					"type": "zip"
				}
			}
		}, {
			"type": "package",
			"package": {
				"name": "resources/angularjs-hint-log",
				"target-dir": "js",
				"version": "0.1.0",
				"dist": {
					"url": "https://github.com/angular/angular-hint-log/archive/v0.4.3.zip",
					"type": "zip"
				}
			}
		}, {
			"type": "package",
			"package": {
				"name": "resources/papa-parse",
				"target-dir": "js",
				"version": "4.1.0",
				"dist": {
					"url": "https://github.com/mholt/PapaParse/archive/4.1.0.zip",
					"type": "zip"
				}
			}
		}
	],

	"require": {
		"xesenix/bdf2-library": "dev-master",
		"resources/jquery": "1.11.*",
		"resources/jquery-ui": "1.11.*",
		"resources/bootstrap": "3.2.*",
		"resources/tinymce": "4.*",
		"resources/vis": "3.11.*",
		"resources/require": "2.1.*",
		"resources/angularjs": "1.3.*",
		"resources/angularjs-hint": "*",
		"resources/angularjs-hint-log": "*",
		"resources/papa-parse": "4.*"
	},

	"autoload": {
		"psr-4": {
			"RPG\\": "src/"
		}
	}
}