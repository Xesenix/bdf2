				"target-dir": "js",
				"version": "2.1.16",
				"dist": {
					"url": "http://requirejs.org/docs/release/2.1.16/minified/require.js",
					"type": "file"
				}
			}
		}, {
			"type": "package",
			"package": {
				"name": "resources/angularjs",
