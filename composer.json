					"url": "http://code.jquery.com/jquery-1.11.1.min.js",
					"type": "file"
				}
			}
		}, {
			"type": "package",
			"package": {
				"name": "resources/jquery-ui",
				"target-dir": "js/jquery-ui",
				"version": "1.11.0",
				"dist": {
					"url": "http://jqueryui.com/resources/download/jquery-ui-1.11.0.zip",
					"type": "zip"
				}
			}
		}, {
			"type": "package",
			"package": {
				"name": "resources/tinymce",
				"version": "4.1.9",
				"dist": {
					"url": "http://download.moxiecode.com/tinymce/tinymce_4.1.9_jquery.zip",
					"type": "zip"
				}
			}
		}, {
			"type": "package",
			"package": {
				"name": "resources/bootstrap",
				"version": "3.3.4",
				"dist": {
					"url": "https://github.com/twbs/bootstrap/releases/download/v3.3.4/bootstrap-3.3.4-dist.zip",
					"type": "zip"
				}
			}
