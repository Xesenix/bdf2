	"repositories": [
		{
            "type": "vcs",
            "url": "https://github.com/Xesenix/Config"
        }, {
			"type": "package",
			"package": {
				"name": "resources/jquery/js/jquery",
				"version": "1.11.1",
				"dist": {
					"url": "http://code.jquery.com/jquery-1.11.1.min.js",
					"type": "file"
				}
			}
		}, {
			"type": "package",
			"package": {
				"name": "resources/jquery-ui/js/jquery-ui",
				"version": "1.11.0",
				"dist": {
					"url": "http://jqueryui.com//resources/download/jquery-ui-1.11.0.zip",
					"type": "zip"
				}
			}
		}, {
			"type": "package",
			"package": {
				"name": "resources/tinymce",
				"version": "4.1.3",
				"dist": {
					"url": "http://download.moxiecode.com/tinymce/tinymce_4.1.3_jquery.zip",
					"type": "zip"
				}
			}
		}, {
            "type": "package",
            "package": {
                "name": "resources/bootstrap",
                "version": "3.2.0",
                "dist": {
                    "url": "https://github.com/twbs/bootstrap/releases/download/v3.2.0/bootstrap-3.2.0-dist.zip",
                    "type": "zip"
                }
            }
        }
	],

	"require": {
		"silex/silex": "~1.2",
		"twig/twig": ">=1.8,<2.0-dev",
		"symfony/twig-bridge": "~2.3",
		"doctrine/orm": "2.4.*",
